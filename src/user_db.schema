create table user (
    username        text primary key,
    password    text,
    can_collect_tweets  text
);

create table oauth (
    username        text primary key,
    consumer_key    text,
    consumer_secret    text,
    token    text,
    token_secret   text
);

create table collection (
    username   text,
    start   text,
    duration    text,
    language    text,
    keyword text,
    location    text,
    user    text,
    running text
);
